{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"target": "ES6",
		"sourceMap": true,
		"inlineSourceMap": false,
		"outDir": "./dist",
		"strictNullChecks": false
	},
	"include": [
        "src/**/*.ts"
    ],
	"exclude": [
		"node_modules"
	]
}
