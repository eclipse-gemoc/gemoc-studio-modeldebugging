<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated scenario -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.gemoc.executionframework.event.model.scenario.presentation.ScenarioModelWizardID"
            name="%_UI_ScenarioModelWizard_label"
            class="org.eclipse.gemoc.executionframework.event.model.scenario.presentation.ScenarioModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ScenarioModelFile.gif">
         <description>%_UI_ScenarioModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated scenario -->
      <editor
            id="org.eclipse.gemoc.executionframework.event.model.scenario.presentation.ScenarioEditorID"
            name="%_UI_ScenarioEditor_label"
            icon="icons/full/obj16/ScenarioModelFile.gif"
            extensions="scenario"
            class="org.eclipse.gemoc.executionframework.event.model.scenario.presentation.ScenarioEditor"
            contributorClass="org.eclipse.gemoc.executionframework.event.model.scenario.presentation.ScenarioActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated property -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.gemoc.executionframework.event.model.property.presentation.PropertyModelWizardID"
            name="%_UI_PropertyModelWizard_label"
            class="org.eclipse.gemoc.executionframework.event.model.property.presentation.PropertyModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PropertyModelFile.gif">
         <description>%_UI_PropertyModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated property -->
      <editor
            id="org.eclipse.gemoc.executionframework.event.model.property.presentation.PropertyEditorID"
            name="%_UI_PropertyEditor_label"
            icon="icons/full/obj16/PropertyModelFile.gif"
            extensions="property"
            class="org.eclipse.gemoc.executionframework.event.model.property.presentation.PropertyEditor"
            contributorClass="org.eclipse.gemoc.executionframework.event.model.property.presentation.PropertyActionBarContributor">
      </editor>
   </extension>
</plugin>
